unit Comon.Debug;

interface

uses windows,
     Config;


  type
    Debug = class
      class procedure outputInfo(DebugInfo:string);
    end;

implementation

{ Debug }

class procedure Debug.outputInfo(DebugInfo: string);
begin
  if Config.TXMLConfig.IsDebug then
    OutputDebugString(PWideChar(DebugInfo));
end;

end.
